{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "vaults_kvTeardown_name": {
            "defaultValue": "kvTeardown",
            "type": "String"
        },
        "virtualNetworks_VNet1_name": {
            "defaultValue": "VNet1",
